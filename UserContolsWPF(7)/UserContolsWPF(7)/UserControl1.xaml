<UserControl x:Class="UserContolsWPF_7_.UserControl1"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:UserContolsWPF_7_"
             mc:Ignorable="d" 
             d:DesignHeight="50" d:DesignWidth="400">
    <Grid x:Name="LayoutRoot" Height="50" Width="400">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="112*"/>
            <ColumnDefinition Width="215*"/>
        </Grid.ColumnDefinitions>
        <TextBox x:Name="FBCTextBox" Margin="4,10,36,4" Text="TextBox" TextWrapping="Wrap" Grid.ColumnSpan="2" TextChanged="FBCTextBox_TextChanged" />
        <Button x:Name="FBCButton" Margin="91,8,13,4" Content="Browse"
                Grid.Column="2" Click="FBCButton_Click" />
    </Grid>
</UserControl>
